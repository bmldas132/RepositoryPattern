@model IEnumerable<ProductApp.core.Product>
@{
    ViewBag.Title = "Home Page";
}
<script src="~/Scripts/jquery-1.10.2.js"></script>

<div class="row">
   
    <div class="col-md-4">
        <h2>Getting started</h2>
        <table class="table-bordered">
            <thead>
                <tr>
                    <td>#</td>
                    <td>Product Id</td>
                    <td>Product Name</td>
                    <td>Price</td>
                    <td>Stock Status</td>
                </tr>
            </thead>
            <tbody>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.id</td>
                        <td> @item.ProdId</td>
                        <td>@item.ProductName</td>
                        <td>@item.ProductPrice</td>
                        <td>@item.Instock</td>
                    </tr>
                }

            </tbody>
        </table>
    </div>

</div>